@import 'reset/reeeset.min.css'
@import 'jquery.fullpage.css'
@import 'jquery.mmenu.all.css'
@import 'font-awesome.min.css'
@import 'simplelightbox.css'
@import 'faq.css'
@import 'faq-block.styl'
@import 'fp-nav.styl'
@import 'modal.styl'
@import 'contact.styl'
@import url('https://fonts.googleapis.com/css?family=Roboto:300,400,700&subset=cyrillic')

$green = #c2db76
$blue = #145bac


@font-face 
	font-family: 'ruble'
	src: url('/fonts/ruble.eot')
	src: url('/fonts/ruble.ttf') format('truetype')
	src: local('ruble'), url('/fonts/ruble.woff') format('woff'), url('/fonts/ruble.ttf') format('truetype')
	font-weight: normal
	font-style: normal

.ruble
	font-family: ruble

body
	font-family: 'Roboto', sans-serif
	font-size: 14px
	color: #333
	background-color: #313237
.action-link
	color: #fff
	text-decoration: none

.clearfix:after {
    content: "";
    display: table;
    clear: both;
}

.btn
	display: inline-block
	border: 2px solid #fff
	background: transparent
	color: #fff
	cursor: pointer
	transform: skew(-10deg)
	margin: 20px 0
	&__text
		display: inline-block
		float: left
		padding: 10px 20px
		border-right: 2px solid #fff
		&-left
			display: block
			transform: skew(10deg)
		&-ruble
			font-family: ruble
	&__icon
		display: inline-block
		transform: skew(10deg)
		padding: 10px 12px
		
	&:hover
	&:focus
		.btn__text
			background-color: #fff
			color: #333
	&_price
		.btn__text
			font-size: 16px
			font-weight: bold
		.btn__icon
			padding: 10px 15px
	&_color
		&_blue
			border-color: $blue
			.btn__icon
				color: $blue
			.btn__text
				border-right: $blue
				background: $blue
				color: #fff
			&:hover
			&:focus
				border-color: lighten( $blue, 10% )
				.btn__text
					background-color: lighten( $blue, 10% )
					border-color: lighten( $blue, 10% )
					color: inherit
					
.content
	position: relative
	float: right
	margin-left: 250px
	background-color: #313237
	width: calc(100% - 250px)
	&_theme
		&_gallery
			padding: 80px 30px
			background-image: url('/img/grunge30.jpg')
			background-repeat: repeat
			background-color: #fff
			min-height: 100vh
			height: 100%
			h1
				color: #444
				text-transform: uppercase
				text-align: center
			p
				text-align: center
				margin-bottom: 25px
				color: #777
			hr
				width: 50px
				border: 0
				height: 1px
				background: #333
				margin: 0px auto 10px auto
			.gallery
				display: block
			.content__buttons
				display: block
				text-align: center
				margin: 30px auto

#cover__video
	display: block
	position: absolute
	top: 0px
	left: 0
	right: 0
	bottom: 0
	opacity: 0.4
	min-width: 100%
	min-height: 100%
	width: auto
	height: auto
	background-size: 100% 100%
	background-color: #000
	background-position: center center
	transform: translateX(0%) translateY(0%)
	background-size: contain
	object-fit: cover
	z-index: -1

.cover
	font-size: 16px
	z-index: 10
	//min-height: 100vh
	background: #000
	color: #fff
	overflow: hidden
	position: relative
	padding: 15px
	&__left
	&__right
		width: 50%
		float: left
		padding: 20px 50px
	&__left
		text-align: right
	&__right
		text-align: left
	&_overlay
		&::before
			display: block
			position: absolute
			height: 100%
			width: 100%
			z-index: 1
			top: 0
			left: 0
			content: ''
			background: #000
			opacity: 0.7
	&__hero
		position: relative
		z-index: 2
		text-align: center

	    img
			margin-bottom: 0px
		hr
			margin: 15px auto
			width: 50px
		p
			color: #ccc
			margin: 20px 0px
	&_block
		&_advantages
			background: url('/img/grunge-dark.png')
			background-position: 50% 50%
			background-size: cover
			.facts
				margin-top: 80px
				margin-bottom: 30px
			.fact
				text-align: center
				float: left
				width: calc((100% / 4) - 5px)
				&__icon
					text-align: center
					margin: auto
					border: 2px solid #fff
					border-radius: 149px
					font-size: 44px
					height: 150px
					width: 150px
					padding: 8px;
					i
						border-radius: 133px
						height: 130px
						width: 130px
						line-height: 130px	
				&__text
					span
						color: #fff
						font-weight: bold
				&:hover
					.fact__icon
						i
							background: #fff
							color: #222	
							transition: background .4s ease

		&_gallery
			background: url('/img/gallery-bg2.jpg')
			background-position: 50% 50%
			background-size: cover
		&_contacts
			background: url('/img/pattern05.png')
			ul
				list-style: none
			.cover__social
				display: block
				width: 100%
				ul li
					display: inline
					a
						width: 30px
						height: 30px
						display: inline-block
						text-align: center
						color: #fff
						border: 2px solid #fff
						margin-left: 10px
						i
							position: relative
							padding-top: 5px
						&:hover
						&:focus
						&:active
							color #bfbfbf
							border-color: #bfbfbf
					&:first-child a
							margin-left: 0px
		&_delivery
			background: url('/img/delivery-bg.jpg')
			background-size: cover
			background-position: 50% 50%
			color: #ccc
			ul
				list-style: none
				padding:0
				margin:0
				color: inherit
				li:before
					font-family: 'FontAwesome'
					margin-right: 8px
					content: '\f1db'
					color: #fff
					font-size: 14px
				li
					span
						color: #fff
						font-weight: bold
			
.call-us
	position: fixed
	top: 0
	right: 0 
	color: #fff
	z-index: 10
	padding: 20px
	font-size: 16px

.email
	position: fixed
	bottom: 0
	right: 0 
	color: #fff
	z-index: 10
	padding: 20px
	font-size: 16px

.sidebar
	background: #313237
	width: 250px
	height: 100vh
	position: fixed
	z-index: 10
	color: #bfbfbf
	font-size: 16px
	box-shadow: 3px 0px 6px 0px rgba(0,0,0,0.5)
	
	&__logo
		padding: 30px 10px 30px 10px
		background: transparent
	&__menu
		background: url('/img/track.png') no-repeat
		background-size: 90%
		background-position: top left
		overflow: visible
		padding: 85px 30px 35px 30px
		list-style: none
		a
			color: #bfbfbf
			/*color: #c2db76*/
			display: block
			line-height: 40px
			height: 40px
			text-decoration: none
			&:hover
				color: #fff
	&__buttons
		padding: 15px 30px 15px 30px
		a
			display: block
			line-height: 60px
			text-align: center
			border: 1px solid #bfbfbf
			border-radius: 15px
			color: #bfbfbf
			text-decoration: none
			margin-bottom: 20px
			&:hover
			&:active
			&:focus
				border-color: $green
				color: #fff
				box-shadow: 0 0 15px rgba(0,0,0,0.5)
	&__links
		display: block
		width: 100%
		position: absolute
		bottom: 0
		text-align: center
		ul li
			display: inline
			a
				width: 30px
				height: 30px
				display: inline-block
				text-align: center
				color: #bfbfbf
				border: 2px solid #bfbfbf
				margin-left: 10px
				i
					position: relative
					padding-top: 5px
				&:hover
				&:focus
				&:active
					color #fff
					border-color: #fff
			&:first-child a
					margin-left: 0px

.mobile-header
	display: none

@media all and (max-width: 992px)
	.sidebar
		display: none
	.content
		margin-left: 0px
		width: 100%
	.mobile-header
		z-index: 100
		display: inline-block
		position: fixed
		padding: 0px 10px
		top: 0
		left: 0
		line-height: 50px
		height: 50px
		width: 100%
		color: #fff
		font-size: 22px
		background: rgba(0, 0, 0, 0.5)
		&__menu-toggle
			float: left
			width: 30%
			text-align: left
			display: inline-block
		&__logo
			width: 40%
			text-align: center
			display: inline-block
			font-weight: bold
		&__phone
			width: 30%
			float: right
			text-align: right
			display: inline-block
		&__phone
		&__menu-toggle
			span
				font-size: 16px	
	.call-us
		display: none
	.email
		display: none
@media all and (max-width: 768px)
	.mobile-header
		&__menu-toggle
		&__phone
			width: 20%
			span
				display: none
		&__logo
			width: 60%

.quater
	width: 50%
	height: 50%
	//min-height: 250px
	background: #fff
	float: left
	&_overlay
		&::before
			display: block
			position: relative
			height: 100%
			width: 100%
			z-index: 1
			top: 0
			left: 0
			content: ''
			background: #000
			opacity: 0.3
	hr
		border: 0
		height: 1px
		background: #333

	h3
		margin-bottom: 5px
		//background-image: url('/img/track-bg2.png')
		//background-repeat: no-repeat
		//background-size: contain
		//background-color: #000

	&__time
		color: $blue

	&__comment
		font-size: 12px
		margin-top: 15px
		color: #999

	&__list
		ul
			list-style: none
			padding:0
			margin:0
			color: inherit
			li:before
				font-family: 'FontAwesome'
				margin-right: 8px
				content: '\f1db'
				color: $green
				font-size: 14px
			li
				span
					font-weight: bold

	&__content
		padding: 25px 30px
		h2
			font-size: 20px
			text-transform: uppercase
			margin-bottom: 10px
		hr
			width: 50px
			margin: 13px 0px

	&_type
		&_cube
			background: url('/img/cube.jpg') no-repeat no-repeat
			background-size: cover
			background-position: 50% 50%
		&_flat
			background: url('/img/flat.jpg') no-repeat no-repeat
			background-size: cover
			background-position: 50% 50%
		&_open-show
			background: url('/img/open-show.jpg') no-repeat no-repeat
			background-size: cover
			background-position: 50% 50%
		&_plus-one
			background: url('/img/plus-one.jpg') no-repeat no-repeat
			background-size: cover
			background-position: 50% 50%
		&_pattern
			background: $green
			background-image: url('/img/pattern05.png')
			background-color: #85c441
			color: #fff
			hr
				border: 0
				height: 1px
				background: #fff
			.quater__time
				color: #fff
			.quater__list
				li:before
					color: #fff
			.quater__comment
				color: #eee

@media all and (max-height: 710px), all and (max-width: 1280px)
	.quater
		&__content
			font-size: 14px
			padding: 20px 25px
			h2
				font-size: 18px
				text-transform: uppercase
				margin-bottom: 10px
			hr
				width: 50px
				margin: 8px 0px
			.quater__comment
				margin-top: 8px
			.btn
				margin-top: 10px
	.cover_block_advantages .facts
		margin-top: 50px
		margin-bottom: 0px
		.fact
			&__text
				font-size: 14px
			
@media all and (max-height: 670px)
	.sidebar__menu
		padding: 75px 30px 0px 30px
	.sidebar__buttons
		padding: 0px 30px 10px 30px

@media all and (max-height: 650px), all and (max-width: 1040px)
	.quater
		&__content
			font-size: 13px
			padding: 10px 15px

@media all and (max-height: 620px), all and (max-width: 1023px)
	.quater
		width: 100%
		&__content
			padding: 70px 35px
	.cover
		&__right
		&__left
			width: 100%
			margin: o auto
			text-align: center
		&__hero
			padding: 120px 0px
	.facts .fact
		text-align: center
		width: 100%
		margin-bottom: 50px

.gallery
	width: 100%
	position: relative
	&__item
		display: block
		width: calc((100% / 3) - 5px)
		float: left
		margin-left: 5px
		img
			width: 100%
			height: auto

@media all and (max-width: 992px)
	.gallery
		&__item
			width: calc((100% / 2) - 5px)

